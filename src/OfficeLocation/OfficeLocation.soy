{namespace OfficeLocation}

/**
 * This renders the component's whole content.
 */
{template .render}
    <div class="officeLocationMainWrapper">
        <div class="officeLocationHeaderText">United Kingdom</div>
        <div class="officeLocationAddress">
            {call IconWithText.render }
					{param iconClass:'fa fa-map-marker'/}
					{param text: 'test Address '/}
                    {param iconClassCss:'icon-with-text-news-box'/}
			{/call}
        </div>
        <div class="officeLocationTel">
             {call IconWithText.render }
					{param iconClass:'fa fa-phone'/}
					{param text: '022-665655566'/}
                    {param iconClassCss:'icon-with-text-news-box'/}
			{/call}
        </div>
        <div class="officeLocationEmail">
             {call IconWithText.render }
					{param iconClass:' fa fa-envelope'/}
					{param text: 'test@test.com'/}
                    {param iconClassCss:'icon-with-text-news-box'/}
			{/call}
        </div>
    </div>
{/template}