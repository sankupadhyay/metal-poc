{namespace StatsItem}

{template .render}
	{@param stats: ?}

	<div class="col-sm-3">
        <div class="card stats-card">
            <div class="card-body">
                <h5 class="card-title">{$stats.values}</h5>
                <p class="card-text">{$stats.label}</p>
            </div>
        </div>
    </div>
{/template}
